{
  "name": "eleventy",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "build:css": "npx tailwindcss -i ./src/styles/tailwind.css -o ./src/assets/style.css",
    "build": "bun cleanup && bun run build:css && npx @11ty/eleventy",
    "start": "bun run build:css --watch & npx @11ty/eleventy --serve",
    "cleanup": "rm -fr dist",
    "dev": "bun cleanup && npx @11ty/eleventy"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "react-dom": "^19.1.1",
    "server": "react-dom/server",
    "tailwindcss": "^4.1.11",
    "tsx": "^4.20.3"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@11ty/eleventy": "^3.1.2"
  }
}
